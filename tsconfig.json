{
  "compilerOptions": {
    "target": "es2019",
    "module": "ESNext",
    "moduleResolution": "nodenext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "out",
    "declaration": true
  },
  "include": ["src"]
}
